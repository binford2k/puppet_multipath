# File managed by puppet, do not modify
#
#
blacklist {
        devnode "*"
}
blacklist_exceptions {
        device {
                vendor  "3PARdata"
                product "VV"
        }
        devnode "^sd[a-z][0-9]*"
}
defaults {
        user_friendly_names yes
        bindings_file /etc/multipath/bindings
        polling_interval 10
        path_grouping_policy    multibus
}
devices {
        device {
                vendor                  "3PARdata"
                product                 "VV"
                no_path_retry           18
                features                "0"
                hardware_handler        "0"
                path_grouping_policy    multibus
                getuid_callout          "/sbin/scsi_id -g -u -s /block/%n"
                path_selector           "round-robin 0"
                rr_weight               uniform
                rr_min_io               100
                path_checker            tur
                failback                immediate
        }
}
